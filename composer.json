{
  "name": "rblinde/html-minifier",
  "type": "grav-plugin",
  "description": "Minifies HTML output of your Grav site",
  "keywords": [
    "grav",
    "plugin",
    "minify",
    "html"
  ],
  "homepage": "https://github.com/rblinde/grav-plugin-html-minifier",
  "license": "MIT",
  "authors": [
    {
      "name": "rblinde",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "wyrihaximus/html-compress": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Grav\\Plugin\\HTMLMinifier\\": "classes/"
    },
    "classmap": [
      "html-minifier.php"
    ]
  },
  "config": {
    "platform": {
      "php": "7.1.3"
    }
  }
}
